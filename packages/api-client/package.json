{
    "name": "@daext/api-client",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs && tsc --emitDeclarationOnly --outDir dist",
        "dev": "tsup src/index.ts --format esm,cjs --watch",
        "lint": "eslint ./src --max-warnings=0",
        "typecheck": "tsc --project tsconfig.json --noEmit"
    },
    "dependencies": {
        "@daext/domain": "workspace:*"
    },
    "devDependencies": {
        "eslint": "^9.30.1",
        "tsup": "^8.3.5",
        "typescript": "~5.8.3",
        "@eslint/js": "^9.30.1",
        "typescript-eslint": "^8.35.1"
    }
}
